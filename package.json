  "main": "index.js",
  "author": "Alex Moon",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "github",
    "ribbon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonmeister/gatsby-plugin-github-ribbon.git"
  },
  "bugs": {
