    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
